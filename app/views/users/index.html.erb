<%= render 'users/user', user: @user%>
<%= render 'books/book', book: @book%>

<h1>Users</h1>
<% @users.each do |user|%>
<div>
  <h2>image</h2>
    <p><%= image_tag user.get_profile_image("40x40")%></p>
  <h2>name</h2>
    <p><%= user.name%></p>
  <h2>詳細</h2>
    <p><%= link_to "Show", user_path(user.id)%></p>
</div>
<%end%>