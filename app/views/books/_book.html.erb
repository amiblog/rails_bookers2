<div class = "container">
  <div class = "row">
    <div class = "col-lg-3">
     <h1>New book</h1>
    <%= form_with model: book, local:true do |f|%>
      <h4>Title</h4>
      <%= f.text_field :title%>
      <h4>Opinion</h4>
      <%= f.text_area :body%>
      <%= f.submit "Create book"%>
    <%end%>
 
    </div>
  </div>
</div>
